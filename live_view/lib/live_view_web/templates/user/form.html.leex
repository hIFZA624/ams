<%= f = form_for @changeset, "#", [phx_change: :validate, phx_submit: @form] %>
<%= hidden_input f, :id %>

<%= label f, :name %>
<%= text_input  f, :name %>
<%= error_tag f, :name %>

<%= label f, :email %>
<%= text_input f, :email %>
<%= error_tag f, :email %>

<div>
    <%= submit @form, phx_disable_with: "Saving..." %>
</div>
</form>
